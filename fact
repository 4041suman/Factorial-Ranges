
class HelloWorld {
    public static void main(String[] args) {
        for(int i=1;i<=50;i++){
          System.out.println(i+" != "+factor(i));
    }
    }
    public static int factor(int n){
        if(n<0){
            return 0;
        }
        if(n<=1){
            return 1;
        }
        return n*factor(n-1);
    }
}
